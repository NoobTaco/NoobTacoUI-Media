{
  "template": "#{{CHANGELOG}}",
  "pr_template": "- #{{TITLE}} (#{{NUMBER}})",
  "empty_template": "",
  "categories": [
    {
      "title": "### Added",
      "labels": ["enhancement", "feature", "added"]
    },
    {
      "title": "### Fixed", 
      "labels": ["bug", "fix", "bugfix", "fixed"]
    },
    {
      "title": "### Changed",
      "labels": ["change", "update", "changed", "improvement"]
    },
    {
      "title": "### Removed",
      "labels": ["removal", "deprecated", "removed"]
    },
    {
      "title": "### Security",
      "labels": ["security"]
    }
  ],
  "ignore_labels": ["duplicate", "question", "invalid", "wontfix", "documentation"],
  "sort": "ASC",
  "max_pull_requests": 200,
  "max_back_track_time_days": 90,
  "uncategorized_title": "### Changed"
}
